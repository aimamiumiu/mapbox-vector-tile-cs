<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard1.3;netstandard2.0</TargetFrameworks>
    <Version>4.0.0</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>mapbox-vector-tile</PackageId>
    <Authors>Bert Temme</Authors>
    <Company />
    <Product />
    <PackageReleaseNotes />
    <Description>.NET library for encoding/decoding Mapbox Vector tiles</Description>
    <RepositoryUrl>https://github.com/bertt/mapbox-vector-tile-cs</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="mapbox.vector.tile.v2.ncrunchproject" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="geojson.net" Version="1.1.64" />
    <PackageReference Include="protobuf-net" Version="2.3.2" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>

</Project>
